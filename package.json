{
  "name": "namespaced-map",
  "version": "0.0.1",
  "description": "A map with namespaced keys that can map to the same value",
  "keywords": ["typescript", "generic", "namespace", "map", "many to one"],
  "author": "leumasic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leumasic/namespaced-map.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/wrapper.mjs",
  "esnext": "dist/mjs/wrapper.mjs",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/wrapper.mjs"
    }
  },
  "files": ["package.json", "README.md", "LICENSE", "dist/"],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest run --passWithNoTests",
    "format": "biome format --write src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
